{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/(.*)",           
        "destination": "/public/index.html"
      },
      {
        "source": "/api/(.*)",            
        "destination": "/api/index.html"
      },
      {
        "source": "/api/game/asahotak",    
        "destination": "/restapi/game/asahotak.js"
      }
    ]
  }